      }
      return res;
    }

    prev() {
      var new_term, new_year;
      new_term = 'fall';
      new_year = this.year;
      if (this.term === 'fall') {
        new_term = 'spring';
      } else {
        new_year -= 1;
      }
      return new YearTerm(new_year, new_term);
    }

    next() {
      var new_term, new_year;
      new_term = 'fall';
      new_year = this.year;
      if (this.term === 'fall') {
        new_term = 'spring';
        new_year += 1;
      }
      return new YearTerm(new_year, new_term);
